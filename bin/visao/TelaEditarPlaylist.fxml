<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="350.0" spacing="30.0" style="-fx-background-color: #14213D;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controle.TelaEditarPlaylistController">
   <Label text="Editar Playlist" textFill="#e5e5e5">
      <font>
         <Font name="Gadugi Bold" size="18.0" />
      </font>
   </Label>
    <TextField fx:id="novoNome" maxWidth="-Infinity" prefHeight="40.0" prefWidth="260.0" promptText="Novo nome" style="-fx-background-color: #14213D; -fx-border-color: white;" styleClass="text-field" stylesheets="@login.css">
      <font>
         <Font size="13.0" />
      </font></TextField>
    <Button maxWidth="100" onAction="#salvarAcao" style="-fx-background-color: white; -fx-background-radius: 20;" text="Salvar" textFill="#14213d">
      <font>
         <Font size="13.0" />
      </font></Button>
</VBox>
